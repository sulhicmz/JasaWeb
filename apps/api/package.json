{
  "name": "@jasaweb/api",
  "version": "1.0.0",
  "description": "NestJS API for JasaWeb Client Portal",
  "type": "commonjs",
  "scripts": {
    "build": "npx nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:cov": "vitest --coverage",
    "test:unit": "vitest --testPathIgnorePatterns=e2e",
    "test:integration": "vitest --config ./test/vitest-e2e.config.ts --testPathPattern=integration",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/vitest --runInBand",
    "test:e2e": "vitest --config ./test/vitest-e2e.config.ts",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.699.0",
    "@aws-sdk/s3-request-presigner": "^3.699.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.1.8",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.9",
    "@types/argon2": "^0.15.4",
    "argon2": "^0.44.0",
    "bcrypt": "^5.1.0",
    "cache-manager": "^7.2.3",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.7.4",
    "handlebars": "^4.7.8",
    "helmet": "^8.0.0",
    "joi": "^18.0.1",
    "luxon": "^3.7.2",
    "nodemailer": "^7.0.7",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.0",
    "redis": "^5.10.0",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/testing": "^11.0.0",
    "@prisma/client": "^7.2.0",
    "@swc/core": "^1.15.5",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.25",
    "@types/luxon": "^3.7.1",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.19.25",
    "@types/nodemailer": "^7.0.3",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^6.0.0",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "prettier": "^3.0.0",
    "prisma": "^7.2.0",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "unplugin-swc": "^1.5.9",
    "vitest": "^2.0.5"
  }
}
