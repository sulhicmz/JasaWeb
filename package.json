{
  "name": "jasaweb",
  "version": "1.0.0",
  "description": "Web berbasis astro untuk manage client",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"pnpm dev:api\" \"pnpm dev:web\"",
    "dev:web": "cd apps/web && pnpm dev",
    "dev:api": "cd apps/api && pnpm start:dev",
    "dev-tools:watch": "./scripts/dev-tools/watch-and-reload.sh all",
    "dev-tools:db": "./scripts/dev-tools/database-tools.sh",
    "dev-tools:quality": "./scripts/dev-tools/code-quality.sh all",
    "prebuild": "pnpm --filter @jasaweb/api exec npx prisma generate",
    "build": "pnpm run build:api && pnpm run build:web",
    "build:web": "cd apps/web && pnpm run build",
    "build:api": "cd apps/api && pnpm run build",
    "start": "pnpm start:api",
    "start:web": "cd apps/web && pnpm start",
    "start:api": "cd apps/api && pnpm start:prod",
    "test": "cd apps/api && pnpm test && cd ../web && pnpm test",
    "test:api": "cd apps/api && pnpm test",
    "test:web": "cd apps/web && pnpm test",
    "lint": "pnpm exec eslint . --max-warnings 500",
    "lint:fix": "pnpm exec eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --build",
    "db:migrate": "cd apps/api && pnpm prisma migrate dev",
    "db:generate": "cd apps/api && pnpm prisma generate",
    "db:seed": "cd apps/api && pnpm prisma db seed",
    "db:studio": "cd apps/api && pnpm prisma studio",
    "db:reset": "cd apps/api && pnpm prisma migrate reset",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs",
    "clean": "rm -rf node_modules apps/*/node_modules packages/*/node_modules dist apps/*/dist packages/*/dist",
    "setup": "./scripts/setup.sh",
    "setup:windows": "echo 'Windows users should use WSL or Git Bash and run: ./scripts/setup.sh'",
    "quick-start": "./scripts/quick-start.sh",
    "security:audit": "pnpm audit --audit-level moderate",
    "security:audit:fix": "pnpm audit --fix",
    "security:check": "pnpm security:audit && pnpm security:outdated",
    "security:outdated": "pnpm outdated",
    "security:scan": "node scripts/security-scan.js",
    "security:generate-env": "node scripts/security/generate-secure-env.ts",
    "security:validate": "node -e \"require('./packages/config/env-validation.ts').validateEnvironmentVariables()\""
  },
  "keywords": [
    "astro",
    "client-portal",
    "websites"
  ],
  "author": "JasaWeb",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitest/ui": "^2.1.9",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-security": "^3.0.1",
    "jsdom": "^27.3.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.3.0",
    "vitest": "^2.1.9"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@8.15.0+sha512.ea45517d5285d123eac02c3793505fa1fd6da90a2fc60d1e8d9e0c1e9292886ecfaff513f062b9d1cc8021bb8615033b1ac5bea3b2ee3fc165a6d7034bbe6b03",
  "pnpm": {
    "overrides": {
      "js-yaml": "4.1.1",
      "nodemailer": "7.0.11",
      "validator": "13.15.22",
      "jws": "3.2.3",
      "esbuild": "0.25.0",
      "body-parser": "2.2.1",
      "mdast-util-to-hast": "13.2.1",
      "html-minifier": "npm:html-minifier-terser@7.2.0",
      "form-data": "2.5.5",
      "tough-cookie": "4.1.3",
      "node-notifier": "8.0.1",
      "braces": "3.0.3",
      "micromatch": "4.0.8",
      "glob": "11.1.0",
      "semver": "7.6.3",
      "word-wrap": "1.2.5",
      "axios": "1.7.9",
      "follow-redirects": "1.15.9",
      "cross-spawn": "7.0.6",
      "path-to-regexp": "8.1.0",
      "undici": "6.21.2",
      "ws": "8.18.0",
      "astro": "5.15.9"
    }
  }
}
