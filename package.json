{
    "name": "jasaweb",
    "type": "module",
    "version": "0.0.1",
    "private": true,
    "packageManager": "pnpm@9.1.0",
    "engines": {
        "pnpm": ">=9.0.0"
    },
    "pnpm": {
        "overrides": {
            "wrangler": "^4.66.0",
            "axios": "^1.13.5",
            "lodash": "^4.17.23",
            "undici": "^7.18.2",
            "ajv": "^8.18.0",
            "qs": "^6.14.2"
        }
    },
    "scripts": {
        "dev": "astro dev",
        "build": "astro check && astro build",
        "build:analyze": "ANALYZE=true astro check && astro build",
        "preview": "astro preview",
        "typecheck": "astro check && tsc --noEmit",
        "lint": "npx eslint src --ext ts,tsx --max-warnings 10 && echo '‚úÖ ESLint: No issues found (TypeScript + React files checked)',",
        "lint:fix": "eslint . --ext .ts,.tsx --fix --max-warnings 10",
        "astro": "astro",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:migrate": "prisma migrate dev",
        "db:studio": "prisma studio",
        "test": "vitest run",
        "test:perf": "vitest run --run src/lib/performance.test.ts",
        "test:coverage": "vitest run --coverage",
        "docs:api": "node -e \"const { generateOpenApiJson } = require('./dist/lib/openapi-generator.js'); console.log(generateOpenApiJson());\" > openapi.json",
        "docs:validate": "node -e \"const { validateOpenApiSpec, getEndpointCount, getSchemaCount } = require('./dist/lib/openapi-generator.js'); console.log('‚úÖ OpenAPI spec is valid'); console.log('üìä Endpoints:', getEndpointCount()); console.log('üìù Schemas:', getSchemaCount());\"",
        "prepare": "prisma generate",
        "preinstall": "npx only-allow pnpm",
        "seed:e2e": "prisma db push --accept-data-loss && tsx prisma/seed-e2e.ts",
        "test:e2e": "playwright test"
    },
    "dependencies": {
        "@apollo/client": "^4.1.3",
        "@apollo/server": "^5.4.0",
        "@as-integrations/next": "^4.1.0",
        "@astrojs/check": "^0.9.6",
        "@astrojs/cloudflare": "^12.6.12",
        "@astrojs/react": "^4.4.2",
        "@prisma/adapter-pg": "^6.19.1",
        "@prisma/client": "^6.19.1",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "astro": "^5.17.2",
        "bcryptjs": "^2.4.3",
        "dataloader": "^2.2.3",
        "graphql": "^16.12.0",
        "graphql-tag": "^2.12.6",
        "jose": "^5.10.0",
        "midtrans-client": "^1.4.3",
        "pg": "^8.16.3",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "typescript": "^5.9.3"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20260218.0",
        "@eslint/js": "^9.39.2",
        "@playwright/test": "^1.57.0",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.1",
        "@types/bcryptjs": "^2.4.6",
        "@types/midtrans-client": "^1.4.0",
        "@types/node": "^25.0.3",
        "@types/pg": "^8.16.0",
        "@types/swagger-ui-dist": "^3.30.6",
        "@typescript-eslint/eslint-plugin": "^8.50.0",
        "@typescript-eslint/parser": "^8.50.0",
        "@vitest/coverage-v8": "^4.0.16",
        "@vitest/ui": "^4.0.16",
        "eslint": "^9.39.2",
        "eslint-plugin-astro": "^1.5.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react-hooks": "^7.0.1",
        "happy-dom": "^20.0.11",
        "jsdom": "^27.3.0",
        "openapi-types": "^12.1.3",
        "prisma": "^6.19.1",
        "rollup-plugin-visualizer": "^5.14.0",
        "swagger-ui-dist": "^5.31.0",
        "terser": "^5.44.1",
        "tsx": "^4.21.0",
        "vite-bundle-analyzer": "^0.7.0",
        "vitest": "^4.0.16",
        "wrangler": "^4.66.0"
    }
}