{
  "name": "jasaweb",
  "version": "1.0.0",
  "description": "Web berbasis astro untuk manage client",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"pnpm dev:api\" \"pnpm dev:web\"",
    "dev:web": "cd apps/web && pnpm dev",
    "dev:api": "cd apps/api && pnpm start:dev",
    "dev-tools:watch": "./scripts/dev-tools/watch-and-reload.sh all",
    "dev-tools:db": "./scripts/dev-tools/database-tools.sh",
    "dev-tools:quality": "./scripts/dev-tools/code-quality.sh all",
    "build": "pnpm build:api && pnpm build:web",
    "build:web": "cd apps/web && pnpm build",
    "build:api": "cd apps/api && pnpm build",
    "start": "pnpm start:api",
    "start:web": "cd apps/web && pnpm start",
    "start:api": "cd apps/api && pnpm start:prod",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:api": "cd apps/api && pnpm test",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "db:migrate": "cd apps/api && pnpm prisma migrate dev",
    "db:generate": "cd apps/api && pnpm prisma generate",
    "db:seed": "cd apps/api && pnpm prisma db seed",
    "db:studio": "cd apps/api && pnpm prisma studio",
    "db:reset": "cd apps/api && pnpm prisma migrate reset",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs",
    "clean": "rm -rf node_modules apps/*/node_modules packages/*/node_modules dist apps/*/dist packages/*/dist",
    "setup": "./scripts/setup.sh",
    "setup:windows": "scripts\\setup.bat",
    "quick-start": "./scripts/quick-start.sh",
    "security:audit": "pnpm audit --audit-level moderate",
    "security:audit:fix": "pnpm audit --fix",
    "security:check": "pnpm security:audit && pnpm security:outdated",
    "security:outdated": "pnpm outdated",
    "security:scan": "node scripts/security-scan.js",
    "postinstall": "node scripts/js-yaml-compat.js"
  },
  "keywords": [
    "astro",
    "client-portal",
    "websites"
  ],
  "author": "JasaWeb",
  "license": "MIT",
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.932.0",
    "@aws-sdk/s3-request-presigner": "^3.932.0",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/testing": "^11.1.9",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.19.0",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.5",
    "@types/html-escaper": "^3.0.4",
    "@types/jest": "^30.0.0",
    "@types/luxon": "^3.7.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/nodemailer": "^7.0.3",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/socket.io-client": "^1.4.36",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.7",
    "@vitest/ui": "^4.0.7",
    "astro": "^5.15.8",
    "bcrypt": "^6.0.0",
    "chart.js": "^4.5.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-security": "^3.0.1",
    "helmet": "^8.1.0",
    "jest": "^30.2.0",
    "luxon": "^3.7.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prettier": "^3.6.2",
    "react-chartjs-2": "^5.3.1",
    "socket.io-client": "^4.8.1",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "uuid": "^13.0.0",
    "vite": "^7.2.2",
    "vitest": "^4.0.7"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "pnpm": {
    "overrides": {
      "js-yaml": "4.1.1",
      "nodemailer": "7.0.7",
      "validator": "13.15.20",
      "html-minifier": "npm:html-minifier-terser@7.2.0"
    }
  }
}
