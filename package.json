{
  "name": "jasaweb",
  "version": "1.0.0",
  "description": "Web berbasis astro untuk manage client",
  "main": "index.js",
  "scripts": {
    "build": "pnpm --recursive run build",
    "build:web": "pnpm --filter @jasaweb/web run build",
    "build:api": "pnpm --filter @jasaweb/api run build",
    "build:ui": "pnpm --filter @jasaweb/ui run build",
    "dev": "pnpm --recursive --parallel run dev",
    "dev:web": "pnpm --filter @jasaweb/web run dev",
    "dev:api": "pnpm --filter @jasaweb/api run start:dev",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.astro --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.astro --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "start": "pnpm build && pnpm --filter @jasaweb/api run start:prod",
    "start:web": "pnpm --filter @jasaweb/web run preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:api": "pnpm --filter @jasaweb/api run test",
    "test:api:cov": "pnpm --filter @jasaweb/api run test:cov",
    "clean": "pnpm --recursive run clean",
    "clean:all": "rm -rf node_modules apps/*/node_modules packages/*/node_modules apps/*/dist packages/*/dist",
    "db:generate": "pnpm --filter @jasaweb/api run prisma:generate",
    "db:migrate": "pnpm --filter @jasaweb/api run prisma:migrate",
    "db:studio": "pnpm --filter @jasaweb/api run prisma:studio",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "astro",
    "client-portal",
    "websites"
  ],
  "author": "JasaWeb",
  "license": "MIT",
  "devDependencies": {
    "@astrojs/eslint-plugin": "^0.0.1",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.0.0",
    "astro-eslint-parser": "^1.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-astro": "^1.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-astro": "^0.12.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}